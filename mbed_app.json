{
    "config": {
        "main-stack-size": {
            "value": 65536
        }
    },
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate": 115200,
            "platform.default-serial-baud-rate": 115200,
            "platform.stdio-convert-newlines": false,
            "platform.minimal-printf-enable-floating-point": true,
            "target.printf_lib": "minimal-printf"
        }
    },
    "requires": ["bare-metal"]
}